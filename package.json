{
  "name": "idoraa-website",
  "version": "0.0.1",
  "description": "",
  "keywords": [
    "Idoraa",
    "Idoraa-Website"
  ],
  "license": "",
  "author": "Saksham",
  "scripts": {
    "tailwind": "tailwindcss -i css/style.css -o css/styles.css --watch",
    "start": "browser-sync start --server --files \"css/*.css,index.html\"",
    "build": "parcel build index.html --public-url ./"
  },
  "devDependencies": {
    "browser-sync": "^2.27.7",
    "parcel-bundler": "^1.12.5",
    "tailwindcss": "^3.0.22"
  },
  "dependencies": {
    "flowbite": "^1.3.3",
    "remove": "^0.1.5"
  },
  "customDistStructure": {
    "config": {
      "imgs": [
        ".jpg",
        ".png",
        ".svg",
        ".gif"
      ],
      "js": [
        ".js",
        ".js.map"
      ],
      "css": [
        ".css",
        ".css.map"
      ]
    },
    "options": {
      "development": true
    }
  }
}
